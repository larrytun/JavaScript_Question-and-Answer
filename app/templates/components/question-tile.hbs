<div class="well">
  <li>
     {{#link-to 'question' question.id}}
     <h2>{{question.title}}</h2>
     <h4>{{question.author}}</h4>
     {{/link-to}}
     {{update-question question=question update="update"}}
  </li>
  <button {{action 'addToFavorites' question}}>Add To Favorites</button>
  <button {{action 'delete' question}}>Delete Question</button>
</div>
